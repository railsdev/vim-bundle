class PatientEducation < ActiveRecord::Base
  has_and_belongs_to_many :products
  has_many :products_patient_educations
  has_many :product, through: :products_patient_educations
  self.primary_key = 'ID' 
  self.table_name  = 'patient_education' 
  alias_attribute  :id, 'ID'
  alias_attribute  :language, 'Language'
  alias_attribute  :sheet_name, 'SHEET_NAME'
  alias_attribute  :descr, 'DESCR'
  alias_attribute  :contra, 'CONTRA'
  alias_attribute  :admin, 'ADMIN'
  alias_attribute  :missed, 'MISSED'
  alias_attribute  :inter, 'INTER'
  alias_attribute  :monitor, 'MONITOR'
  alias_attribute  :side, 'SIDE'
  alias_attribute  :store, 'STORE'
#  self.primary_keys = :id, :language
end
