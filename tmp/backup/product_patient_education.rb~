class ProductPatientEducation < ActiveRecord::Base

  self.table_name  = 'products_patient_education'
  self.primary_key = 'ID'

  alias_attribute  :id, 'ID'
  alias_attribute  :product_id, 'ProductID'
  alias_attribute  :patient_education_id, 'Patient_education_ID'

  belongs_to :product
  belongs_to :patient_education
end
