if @care_giver
json.body do
   json.result "success"
 json.User_Info_List do
       @care_giver.care_taker_id
       json.first_name @care_giver.consumer.first_name
       json.last_name @care_giver.consumer.last_name
       json.email @care_giver.consumer.email
       json.profile_pic @care_giver.consumer.consumer_image
  end
end
else
	json.response_code "200"
	json.response_message "denied"
end