##dosage_diary_remove_user.json.jbuilder
json.response_message "Consumer removed sucessfully"

if @removed_consumer
  json.response_code "200"
  json.response_message ""
  json.body do
    json.result "success"
      json.consumer_id @removed_consumer.consumer_id
      json.first_name @removed_consumer.consumer.first_name
      json.last_name  @removed_consumer.consumer.last_name
      json.email      @removed_consumer.consumer.email
  end

else
  json.response_code "400"
  json.response_message "failure"
  json.result "user not found"
end
