## get_care_giver.json.jbuilder

if @care_givers.present?
  json.response_code "200"
  json.response_message ""
  json.body do
    json.result "success"
    json.care_giver_users @care_givers do |care_giver|
      json.user_id care_giver.id
      json.email_id care_giver.email
      json.first_name care_giver.first_name
      json.last_name care_giver.last_name
      json.profile_picture_url care_giver.consumer_image if care_giver.consumer_image.present?
    end
  end
else
  json.response_code "1000"
  json.response_message "No Records found"
end
