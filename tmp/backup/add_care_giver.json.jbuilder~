##add_care_giver.json.jbuilder
if @rest_response
  json.response_code '0100 '#@rest_response.code
  json.response_message 'Notification sent successfully'
  json.response_url @rest_response.headers[:location]
elsif @user_mail.errors.empty?
  json.response_code "0101"
  json.response_message "Email Sent Successfully"
else
  json.response_code "1000"
  json.response_message "Failure"
end
